    "@types/express-session": "^1.18.0",
    "@types/node": "^22.10.2",
    "@types/react": "^18.3.17",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.4",
    "concurrently": "^9.2.1",
    "autoprefixer": "^10.4.20",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "express-rate-limit": "^8.0.1",
    "express-session": "^1.18.2",
    "groq-sdk": "^0.32.0",
    "helmet": "^8.1.0",
    "lucide-react": "^0.468.0",
    "node-fetch": "^3.3.2",
