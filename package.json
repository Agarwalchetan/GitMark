{
  "name": "advanced-readme-generator",
  "version": "1.0.0",
  "description": "AI-powered README generator with GitHub OAuth authentication",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "client": "vite",
    "server": "nodemon server.js",
